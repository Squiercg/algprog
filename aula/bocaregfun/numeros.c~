#include <stdio.h>

int tamanho(int n) {
  int i=0;

  while(n>0) {
    n=n/10;
    i++;
  }

  return i;

}


int main(void)
{
  int n , a[100] , b[100] , resposta[100]={0} ;

  scanf("%d", &n);

  for(i=0;i<n;i++) {
    scanf("%d %d",&a[i],&b[i]);
  }

  for(i=0;i<n;i++) {
    printf("%d", tamanho(a[i]));
  }





  return 0;
}
