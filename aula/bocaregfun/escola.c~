#include <stdio.h>

typedef struct{
  char nome[101], conceito;
  int rga;
  float p1 , p2 , t1, media;

} registro;

int main(void)
{
  registro aluno[100];

  scanf("%d",&n);

  for(i=0;i<n;i++){
    scanf("%[^\n]",aluno[i].nome);
    scanf("%d",&aluno[i].rga);
    scanf("%d",&aluno[i].p1);
    scanf("%d",&aluno[i].p2);
    scanf("%d",&aluno[i].t1);
  }







  return 0;
}
