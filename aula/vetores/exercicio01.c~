#include <stdio.h>

int main(void)
{
  int v1[100] , v2[100], v3[100] , n, m , i, j, k;

  for(i=0 ; i<n; i++) {
    scanf("%d",&v1[i]);
  }

  for(i=0 ; i<m ; i++) {
    scanf("%d"m &v2[i]);
  }

  i=0; /* v1 */
  j=0; /* v2 */
  k=0; /* v3 */

  while(i<n && j<m) {
    if(v1[i]<v2[j]) {
      v3[k]=v1[i];
      i++;
    } else {
      if(v2[j]<v1[i]) {
	v3[k]=v2[j];
	j++;
      } else {
	v3[k]=v1[i];
	i++;
	j++;
      }
    }
    k++;
  }

