package exe02;

public class Pessoa {
	private String nome;
	private int cpf;
	
	
	
	
	public Pessoa(String nome, int cpf) {
		setNome(nome);
		setCpf(cpf);		
	}
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public int getCpf() {
		return cpf;
	}
	public void setCpf(int cpf) {
		this.cpf = cpf;
	}
	

}
